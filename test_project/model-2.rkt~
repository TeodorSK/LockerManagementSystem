#lang racket/base
(require db)
(require csv-reading)
(require racket/list)

;-----== structs ==------
(struct locker
  (id
   location
   [lockid #:mutable]))

(struct student
  (name))

;-----== definitions ==------



;-----== aux functions ==------
(define (insert-locker a-db new-locker)
  (query-exec
   a-db
   "INSERT INTO lockers (id, location, lockid) VALUES (?, ?, ?)"
   (locker-id new-locker)
   (locker-location new-locker)
   (locker-lockid new-locker)))
   

   